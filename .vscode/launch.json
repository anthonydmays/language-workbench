{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.codedifferently.App",
            "projectName": "codedifferently-app"
        },
        {
            "type": "java",
            "name": "Java Debugger: Current",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "name": "Python Debugger: Python File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "C#: c-sharp",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/c-sharp/c-sharp.csproj",
            "launchConfigurationId": "TargetFramework=;c-sharp"
        }
    ]
}